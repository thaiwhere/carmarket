@model Car.Model.Entity.CarHireEntity

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Đăng tin cho thuê xe";
}

@section Styles{

    <link href="~/Content/css/ajax/ajax-loader.css" rel="stylesheet" />

}

<h2 class="title">&nbsp;&nbsp;@ViewBag.Title</h2>
<hr />
<br />

