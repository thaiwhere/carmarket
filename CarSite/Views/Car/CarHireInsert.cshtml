            @Html.ValidationMessageFor(model => model.Description)
        </div>
    </div>

    <div style="margin-left:10px;">
        <div class="images">
            @{ Html.RenderPartial("~/Views/Shared/UploadFiles.cshtml", null); }
        </div>

        <br />

        <div class="ajax-loader">
            <img src="~/Content/images/common/ajax-loader.gif" class="img-responsive" />
        </div>

        <button type="submit" id="uploadCarHire" class="btn btn-primary btn-lg">Đăng tin</button>
        <br />
        <br />
        @Html.ActionLink("Trở về trang chủ", "Home/Index")
