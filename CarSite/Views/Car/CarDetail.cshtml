@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles{

}
@model List<string>

<div id="div_car_detail" class="car_list_box">

    @{ Html.RenderPartial("~/Views/Shared/SearchingCriteria.cshtml", Model); }

    <div class="car-item-all row-car">

        <div class="car-detail">

            @{ Html.RenderPartial("~/Views/Car/CarDetailImages.cshtml", Model); }
            

        </div>

        <div class="car-detail">            
            <div class="car-detail-section title">
                Tôi cần bán xe Daewoo Laceti sản xuất năm 2009 tên tư nhân xe gia đình sử dụng nên còn chất lượng máy móc êm nội thất đẹp lốp mới đăng kiểm còn dài giá tốt 318 triệu liên hệ Mr Hải 0915558358.
            </div>

        </div>

    </div>
</div>

<div id="car_detai_banner">
    <a target="_blank" href="http://www.hoanggiamobile.com"><img src="/Images/Showroom/hoanggia_1004.gif"></a>
</div>

<div id="div_car_similar" class="div-car-tab">
    <ul class="ul-car-tab">
        <li id="car_similar_model" class="tabactive"><a href="javascript:void(null);" title="Các xe khác cùng model">Các xe khác cùng model</a></li>
        <li id="car_similar_price" class=""><a href="javascript:void(null);" title="Các xe khác có mức giá xấp xỉ (± 10%)">Các xe khác có mức giá xấp xỉ (± 10%)</a></li>
    </ul>

    <div class="grid-container">
        <div id="gridCarSimilar" class="freegrid"></div>
    </div>
</div>

@section Scripts
{
    <script src="~/Content/scripts/CarDetail.js"></script>
}