            <li data-target="#car_detail_carousel" data-slide-to="9"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="/Images/Cars_1/s_cars_1_1.jpg" />
