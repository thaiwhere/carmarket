@model List<string>

<div id="car_detai_images">
    <div id="car_detail_slide">
        <div class="car-active" style="display:none">
            <img id="car_active_item" src="/Images/Cars_1/s_cars_1_1.jpg" width="460" height="445" />
        </div>
        <div class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="/Images/Cars_1/s_cars_1_1.jpg" width="460" height="445" />
                </div>
                <div class="item">
                    <img src="/Images/Cars_1/s_cars_1_2.jpg" width="460" height="445" />
                </div>
                <div class="item">
                    <img src="/Images/Cars_1/s_cars_1_3.jpg" width="460" height="445" />
                </div>
                <div class="item">
                    <img src="/Images/Cars_1/s_cars_1_4.jpg" width="460" height="445" />
                </div>
            </div>
            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
    <div id="car_detail_images_list">
        <span>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_1.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_2.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_3.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_4.jpg">
            </a>
        </span>
    </div>
</div>