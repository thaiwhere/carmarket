@model List<string>


<div id="car_detail_slide">
    <div class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
            <li data-target="#myCarousel" data-slide-to="4"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="/Images/Cars_1/s_cars_1_1.jpg" width="460" height="445" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_2.jpg" width="460" height="445" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_3.jpg" width="460" height="445" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_4.jpg" width="460" height="445" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_5.jpg" width="460" height="445" />
            </div>
        </div>
        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

<div class="car-detail-section img-section">
    <div class="car-detail-section">

        <span>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_1.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_2.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_3.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_4.jpg">
            </a>
            <a href="javascript:void(0)">
                <img src="/Images/Cars_1/s_cars_1_5.jpg">
            </a>
        </span>

    </div>

    <div class="car-detail-section">

        <div>
            <label class="car-label"> Tình trạng </label>
            <span>:&nbsp;Cũ</span>
        </div>
        <div>
            <label class="car-label">Xuất xứ</label>
            <span>:&nbsp;Xuất xứ</span>
        </div>
        <div>
            <label class="car-label">Km</label>
            <span>:&nbsp;40.000 km</span>
        </div>
        <div>
            <label class="car-label">Hộp số</label>
            <span>:&nbsp;Số tự động</span>
        </div>

    </div>

    <div class="car-detail-section">
        <div class="car-price">
            350 triệu
        </div>
        <div class="car-location">
            <a href="/xe-toyota-vios-ha-noi">Hà Nội</a>
        </div>
