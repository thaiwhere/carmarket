            <li data-target="#car_detail_carousel" data-slide-to="6"></li>
            <li data-target="#car_detail_carousel" data-slide-to="7"></li>
            <li data-target="#car_detail_carousel" data-slide-to="8"></li>
            <li data-target="#car_detail_carousel" data-slide-to="9"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="/Images/Cars_1/s_cars_1_1.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_2.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_3.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_4.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_5.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_6.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_7.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_8.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_9.jpg" />
            </div>
            <div class="item">
                <img src="/Images/Cars_1/s_cars_1_10.jpg" />
            </div>
        </div>
        <!-- Left and right controls -->
        <a class="left carousel-control" href="#car_detail_carousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#car_detail_carousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

