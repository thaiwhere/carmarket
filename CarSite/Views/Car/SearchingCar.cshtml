@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles{

}
@model Car.Model.Entity.SearchingType

<div id="div-car-list" class="car_list_box">
    
    @{ Html.RenderPartial("~/Views/Shared/SearchingCriteria.cshtml"); }
    

    <div class="car-list-header">
        <h3>Tin đăng bán xe</h3>
        <span class="car-total">Tổng số : <b>21,287 tin bán xe</b></span>
    </div>
    <!-- begin list car -->
    
    <div class="grid-container">
        <div id="gridSearchingCar" class="freegrid"></div>
    </div>
        
</div>

@section Scripts
{    

    @if (Model.type == 1)
    {
        <script src="~/Content/scripts/SearchingCar.js"></script>
    }
    else if (Model.type == 2)
    {
        <script src="~/Content/scripts/SearchingNewCar.js"></script>
    }
    else
    {
        <script src="~/Content/scripts/SearchingOldCar.js"></script>
    }
    
}
