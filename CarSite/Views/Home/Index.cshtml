@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles{

}
@model List<string>

<div id="div-car-list" class="car-list-box">
    
    @{ Html.RenderPartial("~/Views/Shared/SearchFirm.cshtml", Model); }
    

    <div class="car-list-header">
        <h3>Tin đăng bán xe</h3>
        <span class="car-total">Tổng số : <b>21,287 tin bán xe</b></span>
    </div>
    <!-- begin list car -->
    
